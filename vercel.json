{
    "version": 2,
    "builds": [
      {
      "src": "index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "buildCommand": "npm run vercel-build"
        }
      }
    ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.ts"
    },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}